<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../xsl/command.xsl"?>
<command xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/command.xsd">
  <name>re_trigger_match</name>
  <description>allows to direct a &lt;string&gt; to a trigger &lt;trigger_name&gt;. If this string match regexp, then a corresponding alias is activated.</description>
  <syntax>re_trigger_match &lt;trigger_name&gt; &lt;string&gt;</syntax>
  <arguments>
    <argument>
      <name>trigger_name</name>
      <description>Re_trigger name</description>
    </argument>
    <argument>
      <name>string</name>
      <description>your string</description>
    </argument>
  </arguments>
</command>
