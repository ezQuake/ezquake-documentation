<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../xsl/command.xsl"?>
<command xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/command.xsd">
  <name>re_trigger_delete</name>
  <description>deletes the corresponding trigger.</description>
  <syntax>re_trigger_delete rt_name</syntax>
  <arguments>
    <argument>
      <name>rt_name</name>
      <description>re_trigger name</description>
    </argument>
  </arguments>
</command>
