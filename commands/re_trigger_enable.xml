<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../xsl/command.xsl"?>
<command xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/command.xsd">
  <name>re_trigger_enable</name>
  <description>enables activation of one or more triggers.</description>
  <syntax>re_trigger_enable rt_name1 [rt_name2...]</syntax>
  <arguments>
    <argument>
      <name>rt_name</name>
      <description>Re_trigger name. Can be REGEXP.</description>
    </argument>
  </arguments>
</command>
