<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../xsl/variable.xsl"?>
<variable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/variable.xsd">
  <name>tp_msgtriggers</name>
  <description>Switches on/off message triggers</description>
  <value>
    <boolean>
      <true>Message triggers are on</true>
      <false>Message triggers are off</false>
    </boolean>
  </value>
  <remarks>For ruleset 'Smackdown' message triggers always are off.</remarks>
</variable>
