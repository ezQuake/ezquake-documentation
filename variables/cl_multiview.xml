<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../xsl/variable.xsl"?>
<variable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/variable.xsd">
  <name>cl_multiview</name>
  <description>This client adds a multiview component to mvd playback. Upto four views can be displayed at once. Use this variable to turn multiview on.</description>
  <value>
    <enum>
    <value>
      <name>0</name>
      <description>Multiview disabled</description>
    </value>
    <value>
      <name>1</name>
      <description>Still no multiview but you can use cl_mvdisplayhud to show small complex hud.</description>
    </value>
    <value>
      <name>2</name>
      <description>Two screens.</description>
    </value>
    <value>
      <name>3</name>
      <description>Three screens.</description>
    </value>
    <value>
      <name>4</name>
      <description>Four screens.</description>
    </value>
    </enum>
  </value>
</variable>
