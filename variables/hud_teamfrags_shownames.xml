<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../xsl/variable.xsl"?>
<variable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/variable.xsd">
  <name>hud_teamfrags_shownames</name>
  <description>Draws players' team tags next to frag counts in 'teamfrags' HUD element.</description>
  <value>
    <boolean>
      <true>Display team tags.</true>
      <false>Do not display team tags.</false>
    </boolean>
  </value>
</variable>
