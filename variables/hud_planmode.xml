<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../xsl/variable.xsl"?>
<variable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/variable.xsd">
  <name>hud_planmode</name>
  <description>Toggles special hud-editing mode where all hud elements are being displayed.</description>
  <value>
    <boolean>
      <true>Plan-mode on.</true>
      <false>Plan-mode off.</false>
    </boolean>
  </value>
</variable>
